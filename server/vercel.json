{
    "version": 2,
    "builds": [
        {
        "src" : "servers.js",
        "use": "@vercel/node",
        "config": {
            "includeFiles": [
                "dist/**"
            ]
        }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}